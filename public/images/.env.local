# .env.local (in your project root)
# Google Service Account Credentials
GOOGLE_TYPE=service_account
GOOGLE_PROJECT_ID=your-project-id-123
GOOGLE_PRIVATE_KEY_ID=your-private-key-id-abc123
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour-actual-private-key-with-escaped-newlines\n-----END PRIVATE KEY-----\n"
GOOGLE_CLIENT_EMAIL=your-service-account@project-id.iam.gserviceaccount.com
GOOGLE_CLIENT_ID=123456789012345678901
GOOGLE_AUTH_URI=https://accounts.google.com/o/oauth2/auth
GOOGLE_TOKEN_URI=https://oauth2.googleapis.com/token
GOOGLE_AUTH_PROVIDER_X509_CERT_URL=https://www.googleapis.com/oauth2/v1/certs
GOOGLE_CLIENT_X509_CERT_URL=https://www.googleapis.com/robot/v1/metadata/x509/your-service-account%40project-id.iam.gserviceaccount.com

# Google Drive Configuration
GOOGLE_DRIVE_FOLDER_ID=your-folder-id-abc123
